{
  "Language": "French",
  "ElementIdToFindInHomePage": "username",
  "WebsiteUrl": "https://lysrv-ovetest-110.oikialog.local.:5001/webapp/",

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "Logs/log.db",
          "tableName": "Log"
        }
      },
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.json",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  }
}